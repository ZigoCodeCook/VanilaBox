 </div>\n{% endif %}\n\n{% if collection.handle == 'moken-all' %}\n  <style>\n    .circle-subcategory {\n      width: 90px;\n      font-size: 14px;\n    }\n\n    .circle-subcategory img {\n      width: 75px;\n      height: 75px;\n    }\n\n    @media screen and (min-width: 768px) {\n      .circle-subcategory {\n        width: 120px;\n        font-size: 16px;\n      }\n\n      .circle-subcategory img {\n        width: 110px;\n        height: 110px;\n      }\n    }\n  </style>\n\n  <div class=\"circle-subcategories\">\n    <a href=\"/collections/moken-all?constraint=collection:fashion\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/35.jpg?v=1753624253\" alt=\"Fashion\">\n      Fashion\n    </a>\n    <a href=\"/collections/moken-all?constraint=collection:active\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/17_1.jpg?v=1753637981\" alt=\"Active\">\n      Active\n    </a>\n    <a href=\"/collections/moken-all?constraint=collection:kids\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/4_3.jpg?v=1753686760\" alt=\"Kids\">\n      Kids\n    </a>\n  </div>\n{% endif %}"
