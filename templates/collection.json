    "custom_liquid_PxKFWM": {
      "type": "custom-liquid",
      "name": "Sub-categories",
      "settings": {
        "use_container": true,
        "custom_liquid": "{% if collection.handle == 'sunbum-all' or collection.handle == 'original-sun-care' or collection.handle == 'hair-care' or collection.handle == 'original-lip-balm' or collection.handle == 'merch' or collection.handle == 'moken-all' or collection.handle == 'fashion-sunglasses-by-moken' or collection.handle == 'moken-sports' or collection.handle == 'moken-kids' or collection.handle == 'shop-all-aloha' or collection.handle contains 'aloha-' %}\n  <style>\n    .circle-subcategories {\n      display: flex;\n      gap: 12px;\n      flex-wrap: wrap;\n      justify-content: center;\n      margin: 20px 0;\n    }\n\n    .circle-subcategory {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      text-align: center;\n      text-decoration: none;\n      color: #333;\n      font-weight: bold;\n      width: 70px;\n      font-size: 14px;\n    }\n\n    .circle-subcategory img {\n      width: 55px;\n      height: 55px;\n      object-fit: cover;\n      border-radius: 50%;\n      margin-bottom: 6px;\n      border: 1px solid #ccc;\n    }\n\n    @media screen and (min-width: 768px) {\n      .circle-subcategory {\n        width: 110px;\n        font-size: 15px;\n      }\n\n      .circle-subcategory img {\n        width: 100px;\n        height: 100px;\n      }\n    }\n\n    /* ALOHA horizontal scroll styles */\n    .aloha-carousel {\n      display: flex;\n      overflow-x: auto;\n      gap: 12px;\n      padding: 10px 12px;\n      scroll-snap-type: x mandatory;\n      -webkit-overflow-scrolling: touch;\n    }\n\n    .aloha-carousel::-webkit-scrollbar {\n      display: none;\n    }\n\n    .aloha-carousel .circle-subcategory {\n      flex: 0 0 auto;\n      scroll-snap-align: start;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      text-align: center;\n      text-decoration: none;\n      color: #333;\n      font-weight: bold;\n      width: 80px;\n      font-size: 13px;\n    }\n\n    .aloha-carousel .circle-subcategory img {\n      width: 60px;\n      height: 60px;\n      object-fit: cover;\n      border-radius: 50%;\n      margin-bottom: 6px;\n      border: 1px solid #ccc;\n    }\n\n    @media screen and (min-width: 768px) {\n      .aloha-carousel .circle-subcategory {\n        width: 110px;\n        font-size: 15px;\n      }\n\n      .aloha-carousel .circle-subcategory img {\n        width: 100px;\n        height: 100px;\n      }\n    }\n  </style>\n{% endif %}\n\n{%- comment -%} SUN BUM {%- endcomment -%}\n{% if collection.handle contains 'sunbum' or collection.handle == 'original-sun-care' or collection.handle == 'hair-care' or collection.handle == 'original-lip-balm' or collection.handle == 'merch' %}\n  <div class=\"circle-subcategories\">\n    <a href=\"/collections/original-sun-care\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/35.png?v=1736771916\" alt=\"Sun care\">\n      Sun care\n    </a>\n    <a href=\"/collections/hair-care\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/28.png?v=1712225668\" alt=\"Hair care\">\n      Hair care\n    </a>\n    <a href=\"/collections/original-lip-balm\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/11.png?v=1736765224\" alt=\"Lip care\">\n      Lip care\n    </a>\n    <a href=\"/collections/merch\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/14.png?v=1712225418\" alt=\"Merch\">\n      Merch\n    </a>\n  </div>\n{% endif %}\n\n{%- comment -%} MOKEN {%- endcomment -%}\n{% if collection.handle contains 'moken' %}\n  <div class=\"circle-subcategories\">\n    <a href=\"/collections/fashion-sunglasses-by-moken\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/35.jpg?v=1753624253\" alt=\"Fashion\">\n      Fashion\n    </a>\n    <a href=\"/collections/moken-sports\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/17_1.jpg?v=1753637981\" alt=\"Action\">\n      Action\n    </a>\n    <a href=\"/collections/moken-kids\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/4_3.jpg?v=1753686760\" alt=\"Kids\">\n      Kids\n    </a>\n    <a href=\"https://vanillabox.co/collections/ski-collection\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/TWOGKN02-BLU-SLV.jpg?v=1757576682\" alt=\"Ski\">\n      Ski\n    </a>\n  </div>\n{% endif %}\n\n{%- comment -%} ALOHA (horizontal scroll carousel) {%- endcomment -%}\n{% if collection.handle contains 'aloha-' or collection.handle == 'shop-all-aloha' %}\n  <div class=\"aloha-carousel\">\n    <a href=\"/collections/aloha-tote-bags\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/DT_heipua_sorbet_on_lavande_3.png?v=1746520362\" alt=\"Tote Bags\">\n      <span>Tote Bags</span>\n    </a>\n    <a href=\"/collections/aloha-pouches\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/sm_petite_check_latte_1_6b8b00a8-12e3-4ae7-8720-9864e9ca5e0b.png?v=1746521825\" alt=\"Pouches\">\n      <span>Pouches</span>\n    </a>\n    <a href=\"/collections/aloha-crossbody-bags\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/DT_petite_check_latte_1.png?v=1746522075\" alt=\"Crossbody\">\n      <span>Crossbody</span>\n    </a>\n    <a href=\"/collections/aloha-backpacks\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/nylon_backpack_monochrome_lavande_1.png?v=1746522795\" alt=\"Backpacks\">\n      <span>Backpacks</span>\n    </a>\n    <a href=\"/collections/aloha-weekender-bags\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/nylon_weekender_monochrome_grove_1.png?v=1746522666\" alt=\"Weekenders\">\n      <span>Weekenders</span>\n    </a>\n    <a href=\"/collections/aloha-dopp-kits\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/dopp_heipua_sorbet_lavande_1.png?v=1746520300\" alt=\"Dopp kits\">\n      <span>Dopp kits</span>\n    </a>\n    <a href=\"/collections/aloha-hip-packs\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/nylon_hip_monochrome_lavande_1.png?v=1746523032\" alt=\"Hip packs\">\n      <span>Hip packs</span>\n    </a>\n    <a href=\"/collections/aloha-duffle-bags\" class=\"circle-subcategory\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0692/0257/5613/files/duffle_samudra_kalapana.png?v=1746514141\" alt=\"Duffles\">\n      <span>Duffles</span>\n    </a>\n  </div>\n{% endif %}"
      }
